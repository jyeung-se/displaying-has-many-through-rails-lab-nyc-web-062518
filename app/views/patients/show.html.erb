<h1><%= @patient.name %></h1>

<ul>
  <% @patient.appointments.each do |appointment| %>
    <li>Appointment: <%= appointment.appointment_datetime.strftime("%B %d, %Y at %H:%M") %></li>
    <li>Doctor: Dr. <%= link_to appointment.doctor.name, doctor_path(appointment.doctor) %></li><br>
  <% end %>
</ul>
